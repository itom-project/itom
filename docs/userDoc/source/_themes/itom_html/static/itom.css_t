@import url("basic.css");

html {
    margin: 0px;
    padding: 0px;
    background: #FFF url(bg-page.png) top left repeat-x;
}

body {
    line-height: 1.5;
    margin: auto;
    padding: 0px;
    font-family: "DejaVu Sans", Arial, Helvetica, sans-serif;
    min-width: 59em;
    max-width: 70em;
    color: #000000;
    border-style:double;
    border-width: 0px;
}

/*---------------------------------------Von Martin -------------------------------------------*/
/*--------------------------------------------------------------------------------------------------------*/
/*Parameters, Returns, etc - Spalte*/
col.field-name{
    background-color: #F6E3CE;
    overflow: hidden;
	width: 115px;
    border-left: 2px solid #000000;
}

/*class.itom ... Ueberschrift*/
dl.class dt{
   font-size: 19px;
}

/*Das Wort "class" in der Klassenueberschrift*/
dl.class dt em.property{
	font-size: 1.05em
}


/*Hinterlegung der gesamten Methoden*/
dl.method, dl.staticmethod, dl.function{
  background-color: #ffffff; /*#FBF8EF;*/
  border: 0px solid #000000;
}

/*Hinterlegung des gesamten Attributes*/
dl.attribute{
  background-color: #F2FBff;
  border: 1px solid;
}

/*Hinterlegung des Attributeheaders*/
dl.attribute dt{
 	background-color: #E6E6E6;
  font-size: 13px;
  padding-left: 7px;
}

/*Hinterlegung des static- und Mehodenheaders*/
dl.method dt, dl.staticmethod dt, dl.function dt{
 	background-color: #E6E6E6;
  border-top: 1px solid #000000; /*#B40404*/
  font-size: 13px;
  padding-left: 7px;
}

/*---------------------------------------Von Martin-ENDE--------------------------------------*/
/*--------------------------------------------------------------------------------------------------------*/

div.footer {
    padding: 10px;
    font-size: 11px;
    text-align: center;
    letter-spacing: 0.5px;
}

/* link colors and text decoration */

a:link {
    font-weight: bold;
    text-decoration: none;
    color: #dc3c01;
}

a:visited {
    font-weight: bold;
    text-decoration: none;
    color: #892601;
}

a:hover, a:active {
    text-decoration: underline;
    color: #ff4500;
}

/* Some headers act as anchors, don't give them a hover effect */

h1 a:hover, a:active {
    text-decoration: none;
    color: #0c3762;
}

h2 a:hover, a:active {
    text-decoration: none;
    color: #0c3762;
}

h3 a:hover, a:active {
    text-decoration: none;
    color: #0c3762;
}

h4 a:hover, a:active {
    text-decoration: none;
    color: #0c3762;
}

a.headerlink {
    color: #a7ce38;
    padding-left: 5px;
}

a.headerlink:hover {
    color: #a7ce38;
}

/* basic text elements */

div.content {
    margin-top: 20px;
    margin-left: 40px;
    margin-right: 40px;
    margin-bottom: 50px;
    font-size: 0.9em;
}

/* heading and navigation */

div.content.div#dataobject.section.dl.class.dd.table.docutils.field-list.tbody.tr.field-odd.field.th.field-name{
  backround: #0000FF;
}

div.header {
    position: relative;
    left: 0px;
    top: 0px;
    height: 85px;
    background: #efefef; */
    padding: 0 40px;
}
div.header h1 {
    font-size: 1.6em;
    font-weight: normal;
    letter-spacing: 3px;
    color: #0c3762;
    border: 0;
    margin: 0;
    padding-top: 15px;
    padding-left: 10px;
}
div.header h1 a {
    font-weight: normal;
    color: #0c3762;
}
div.header h2 {
    font-size: 1.3em;
    font-weight: normal;
    letter-spacing: 1px;
    text-transform: uppercase;
    color: #aaa;
    border: 0;
    margin-top: -3px;
    padding: 0;
    padding-left: 20px;
}

div.header img.rightlogo {
    float: right;
}


div.title {
    font-size: 1.3em;
    font-weight: bold;
    color: #0c3762;
    border-bottom: dotted thin #e0e0e0;
    margin-bottom: 25px;
}
div.topnav {
    background: #e0e0e0; */
}
div.topnav p {
    margin-top: 0;
    margin-left: 40px;
    margin-right: 40px;
    margin-bottom: 0px;
    text-align: right;
    font-size: 0.8em;
}
div.bottomnav {
    background: #e0e0e0;
}
div.bottomnav p {
    margin-right: 40px;
    text-align: right;
    font-size: 0.8em;
}

a.uplink {
    font-weight: normal;
}


/* contents box */

table.index {
    margin: 0px 0px 30px 30px;
    padding: 1px;
    border-width: 1px;
    border-style: dotted;
    border-color: #e0e0e0;
}
table.index tr.heading {
    background-color: #e0e0e0;
    text-align: center;
    font-weight: bold;
    font-size: 1.1em;
}
table.index tr.index {
    background-color: #eeeeee;
}
table.index td {
    padding: 5px 20px;
}

table.index a:link, table.index a:visited {
    font-weight: normal;
    text-decoration: none;
    color: #dc3c01;
}
table.index a:hover, table.index a:active {
    text-decoration: underline;
    color: #ff4500;
}


